{
    "name": "dynamic/silverstripe-site-notifications",
    "description": "Display site notifications like violators and pop ups",
    "type": "silverstripe-vendormodule",
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dyamicagency.com",
            "homepage": "https://www.dynamicagency.com"
        }
    ],
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/linkfield":"^3.0",
        "silverstripe/recipe-cms": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\SiteMessages\\": "src/",
            "Dynamic\\SiteMessages\\Test\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/js"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
